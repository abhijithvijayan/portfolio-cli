{
	"name": "abhijithvijayan-portfolio",
	"version": "1.8.3",
	"description": "CLI to use my portfolio site as a template to build yours!",
	"license": "MIT",
	"author": "abhijithvijayan",
	"bin": {
		"abhijithvijayan-portfolio": "app/index.js"
	},
	"files": [
		"app",
		"demo.gif"
	],
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
