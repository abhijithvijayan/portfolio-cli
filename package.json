{
	"name": "@abhijithvijayan/portfolio",
	"version": "1.2.1",
	"description": "CLI to use my portfolio site as a template to build yours!",
	"license": "MIT",
	"author": "abhijithvijayan",
	"engines": {
		"node": ">=10.0.0"
	},
	"bin": "source/index.js",
	"files": [
		"source"
	],
	"scripts": {
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
