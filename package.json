	"dependencies": {
		"chalk": "^3.0.0",
		"del": "^5.1.0",
		"execa": "^3.4.0",
		"inquirer": "^7.0.3",
		"meow": "^5.0.0",
		"ora": "^4.0.3",
		"validate.io-object": "^1.0.4"
	},
	"devDependencies": {
		"@abhijithvijayan/eslint-config": "1.4.0",
		"@typescript-eslint/eslint-plugin": "^2.31.0",
		"@typescript-eslint/parser": "^2.31.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.1.0",
